{
  "name": "yuncms/yii2-article",
  "description": "The article extension for the yuncms",
  "keywords": [
    "yuncms",
    "article"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-queue": "~2.0.0",
    "xutl/yii2-fileupload-widget": "~1.0",
    "yuncms/yii2-ueditor-widget": "~1.0.0",
    "yuncms/yii2-user": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "yuncms\\article\\": ""
    }
  },
  "extra": {
    "yuncms": {
      "name": "article",
      "i18n": {
        "class": "yii\\i18n\\PhpMessageSource",
        "sourceLanguage": "en-US",
        "basePath": "@yuncms/article/message"
      },
      "migrationNamespace": "yuncms\\article\\migrations",
      "backend": {
        "class": "yuncms\\article\\backend\\Module"
      },
      "frontend": {
        "class": "yuncms\\article\\frontend\\Module"
      }
    },
    "branch-alias": {
      "dev-master": "2.0-dev"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  ]
}
